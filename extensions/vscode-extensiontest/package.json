{
  "name": "vscode-testextension",
  "description": "nick's test vscode extension",
  "author": "Sourcegraph",
  "license": "MIT",
  "version": "0.0.1",
  "publisher": "sourcegraph",
  "engines": {
    "vscode": "^1.6.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:go"
  ],
  "configuration": {
    "type": "object",
    "title": "Go configuration",
    "properties": {
      "resultCount": {
        "type": "number",
        "default": 100,
        "description": "Number of results to return"
      }
    }
  },
  "main": "./out/extension",
  "contributes": {
    "languages": [
      {
        "id": "go",
        "extensions": [
          ".go"
        ],
        "aliases": [
          "Go"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "update-vscode": "node ./node_modules/vscode/bin/install",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "isolated-vscode": "npm run compile && VSCODE=$(which code-insiders || which code || echo echo ERROR: neither the code nor code-insiders vscode executable is installed); USER=dummy-dont-share-vscode-instance $VSCODE --user-data-dir=$PWD/.vscode-dev/user-data --extensionHomePath=$PWD/.vscode-dev/extensions --extensionDevelopmentPath=$PWD $*"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.45",
    "typescript": "^2.1.5",
    "vscode": "^1.0.0"
  },
  "dependencies": {
    "vscode-languageclient": "^3.0.3"
  }
}